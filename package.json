{
  "name": "moment-hooks",
  "version": "1.0.0",
  "description": "Incredibly awesome hooks",
  "keywords": [
    "react",
    "react-hooks",
    "momentech",
    "hooks",
    "query"
  ],
  "author": "Max Moeschinger",
  "license": "MIT",
  "homepage": "https://github.com/momentechnologies/moment-hooks#readme",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "test": "jest --config jest.config.json",
    "build": "rm -rf lib/ && tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/momentechnologies/moment-hooks.git"
  },
  "bugs": {
    "url": "https://github.com/momentechnologies/moment-hooks/issues"
  },
  "devDependencies": {
    "@types/jest": "^24.0.25",
    "@types/react": "^16.9.17",
    "@types/react-router-dom": "^5.1.3",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "ts-jest": "^24.3.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "query-string": "^6.9.0"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  }
}
